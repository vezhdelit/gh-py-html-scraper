{
"version": "1.0.0",
"manifest_version": 3,
"name": "Chrome Proxy",
"permissions": [
    "proxy",
    "tabs",
    "unlimitedStorage",
    "storage",
    "webRequest",
    "webRequestAuthProvider"
],
"host_permissions": [
    "<all_urls>"
],
"background": {
    "service_worker": "background.js"
},
"minimum_chrome_version":"88.0.0"
}